{
    "version": 2,
    "settings": {
        "color": {
            "link": true,
            "palette": [
                {
                    "slug": "red",
                    "color": "#fd1a35",
                    "name": "Red"
                },
                {
                    "slug": "white",
                    "color": "#ffffff",
                    "name": "White"
                },
                {
                    "slug": "gray",
                    "color": "#eeeeee",
                    "name": "Gray"
                },
                {
                    "slug": "jet-black",
                    "color": "#3F3F3F",
                    "name": "Jet Black"
                }
            ]
        },
        "layout":{
            "contentSize": "1180px",
			"wideSize": "1380px"
        },
        "typography": {
			"fontSizes": [
				{
					"slug": "small",
					"size": "1rem",
					"name": "Small"
				},
				{
					"slug": "medium",
